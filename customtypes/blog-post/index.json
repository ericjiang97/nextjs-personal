{
  "id": "blog-post",
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "uid"
        }
      },
      "title": {
        "type": "StructuredText",
        "config": {
          "single": "heading1",
          "label": "title"
        }
      },
      "author": {
        "type": "StructuredText",
        "config": {
          "single": "heading2",
          "label": "author",
          "placeholder": "Eric Jiang <eric@ericjiang.dev>"
        }
      },
      "published_time": {
        "type": "Timestamp",
        "config": {
          "label": "published_time"
        }
      },
      "preview": {
        "type": "Boolean",
        "config": {
          "default_value": false,
          "label": "preview"
        }
      },
      "category": {
        "type": "Link",
        "config": {
          "select": "document",
          "customtypes": ["category"],
          "label": "category"
        }
      },
      "summary": {
        "type": "StructuredText",
        "config": {
          "single": "paragraph",
          "label": "summary"
        }
      },
      "banner": {
        "type": "Image",
        "config": {
          "constraint": {},
          "thumbnails": [],
          "label": "banner"
        }
      },
      "body": {
        "type": "StructuredText",
        "config": {
          "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl",
          "label": "body"
        }
      },
      "body1": {
        "type": "Slices",
        "fieldset": "Slice zone",
        "config": {
          "labels": {},
          "choices": {
            "before_after": {
              "type": "SharedSlice"
            }
          }
        }
      }
    }
  },
  "label": "blog-post",
  "repeatable": true,
  "status": true,
  "format": "custom"
}
